<robot name="maggie">
		
	
        <link name="base_link">
		<visual>
                        <origin xyz="0 0 0" />
			<geometry>
                                <cylinder radius="0.20" length="1.02" />
			</geometry>
			<material name="blue">
				<color rgba="0 0.5 1 1"/>
			</material>
		</visual>		
	</link>

	
	<link name="neck_link">
		<visual>
                        <origin xyz="0 0 -1.22" />
			<geometry>
                                <cylinder radius="0.0135" length=".077" />
			</geometry>
			<material name="white">
				<color rgba="1 1 1 1"/>
			</material>
		</visual>
	</link>
	
	<link name="head_link">
		<visual>
                        <origin xyz="0 0 -0.03" />
			<geometry>
				<box size=".01 .07 .05" />
			</geometry>
			<material name="white">
				<color rgba="1 1 1 1"/>
			</material>
		</visual>	
	</link>

        <!-- regarding head -->
        <link name="kinect_head_link">
                <visual>
                        <origin xyz="0 0 -0.20" />
                        <geometry>
                                <box size=".28 .04 .06" />
                        </geometry>
                        <material name="darkgrey">
                                <color rgba="0.3 0.3 0.3 1"/>
                        </material>
                </visual>
        </link>

        <!-- regarding base -->
        <link name="kinect_base_link">
                <visual>
                        <origin xyz="0 0 -0.50" />
                        <geometry>
                                <box size=".28 .04 .06" />
                        </geometry>
                        <material name="darkgrey">
                                <color rgba="0.3 0.3 0.3 1"/>
                        </material>
                </visual>
        </link>


        <!-- regarding base -->
	<link name="leftArm_link">
		<visual>
                        <origin xyz="0 0 -0.94" />
			<geometry>
                                <box size="0.07 0.12 0.37" />
			</geometry>
			<material name="white">
				<color rgba="1 1 1 1"/>
			</material>
		</visual>	
	</link>
	
        <!-- regarding base -->
	<link name="rightArm_link">
		<visual>
                        <origin xyz="0 0 -0.94" />
			<geometry>
                                <box size="0.07 0.12 0.37" />
			</geometry>
			<material name="white">
				<color rgba="1 1 1 1"/>
			</material>
		</visual>	
	</link>

        <!-- JOINTS -->

        <joint name="neck" type="revolute">
                <parent link="base_link"/>
                <child link="neck_link"/>
                <origin xyz="0 0 -1.22" rpy="0 0 0" />
                <axis xyz="0 0 1" />
                <limit lower="-1.57" upper="1.57" effort="10" velocity="3" />
        <dynamics damping="0.0"/>
        </joint>

        <joint name="head" type="revolute">
                <parent link="neck_link"/>
                <child link="head_link"/>
                <origin xyz="0 0 -0.03" rpy="1.57 0 0" />
                <axis xyz="0 -1 0" />
                <limit lower="-1.57" upper="1.57" effort="10" velocity="3" />
        <dynamics damping="0.0"/>
        </joint>

	
        <joint name="kinect_head" type="fixed">
                <parent link="head_link"/>
                <child link="kinect_head_link"/>
                <origin xyz="0 0 -0.20" rpy="1.57 0 0" />
	</joint>


        <joint name="kinect_base" type="fixed">
                <parent link="base_link"/>
                <child link="kinect_base_link"/>
                <origin xyz="0 0 -0.49" rpy="1.83 0 0" />
        </joint>

	<joint name="rightArm" type="revolute">
		<parent link="base_link"/>
		<child link="rightArm_link"/>
                <origin xyz="0.16 0 -0.94" rpy="1.57 0 0" />
		<axis xyz="0 -1 0" />
		<limit lower="-1.57" upper="1.57" effort="10" velocity="3" />
      	<dynamics damping="0.0"/>
	</joint>
	
	<joint name="leftArm" type="revolute">
		<parent link="base_link"/>
		<child link="leftArm_link"/>
                <origin xyz="-0.16 0 -0.94" rpy="-1.57 0 0" />
		<axis xyz="0 -1 0" />
		<limit lower="-1.57" upper="1.57" effort="10" velocity="3" />
      	<dynamics damping="0.0"/>
	</joint>


</robot>

