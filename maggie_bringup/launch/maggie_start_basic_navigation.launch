<?xml version="1.0" encoding="UTF-8"?>

<launch>
    <arg name="robot" value="maggie" />

    <!-- DEVICES - -->

    <!-- LABJACK -->
    <include file="$(find maggie_labjack)/launch/labjack.launch">
        <arg name="robot" value="$(arg robot)" />
    </include>

    <!-- SKILLS - -->

    <!-- STATES -->
    <include file="$(find basic_states_skill)/launch/basic_states_skill.launch">
        <arg name="robot" value="$(arg robot)" />
    </include>

    <!-- TOUCH -->
    <include file="$(find touch_skill)/launch/touch_skill.launch">
        <arg name="robot" value="$(arg robot)" />
    </include>

    <!-- GOTO PLACE -->
    <include file="$(find goto_place_skill)/launch/goto_place_skill.launch">
        <arg name="robot" value="$(arg robot)" />
    </include>

    <!-- NAVIGATION USING MAP -->
    <include file="$(find maggie_bringup)/launch/maggie_navigation.launch" />
</launch>
